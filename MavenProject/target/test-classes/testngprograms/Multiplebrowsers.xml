<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite" parallel="none">
 	<test name="microedge">
 		<parameter name="browser" value="microedge" />
 		<classes>
 			<class name="testngprograms.Multiplebrowsers" />
 		</classes>
 	</test>
 	<test name="ChromeTest">
 		<parameter name="browser" value= "chrome" />
 		<classes>
 			<class name="testngprograms.Multiplebrowsers" />
 		</classes>
 	</test>
 	<test name="Firefox">
 		<parameter name="browser" value= "Firefox" />
 		<classes>
 			<class name="testngprograms.Multiplebrowsers" />
 		</classes>
 	</test>
</suite>
